    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/537make",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/simple_testcase",
            "environment": [],
